{
  "name": "Python 3",
  "image": "mcr.microsoft.com/devcontainers/python:1-3.11-bullseye",
  "customizations": {
    "codespaces": {
      "openFiles": [
        "README.md",
        "index.html"
      ]
    },
    "vscode": {
      "settings": {},
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance"
      ]
    }
  },
  "postCreateCommand": "pip3 install --user -r requirements.txt",
  "portsAttributes": {
    "5000": {
      "label": "Trading Scanner",
      "onAutoForward": "openPreview"
    }
  },
  "forwardPorts": [
    5000
  ]
}